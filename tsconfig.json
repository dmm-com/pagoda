    "moduleResolution": "node",
    "strict": true,
    "skipLibCheck": true,
    "jsx": "react-jsx",
    "lib": ["dom", "dom.iterable", "es2022"],
    "esModuleInterop": true
  },
  "include": ["./frontend/src/**/*"]
}
