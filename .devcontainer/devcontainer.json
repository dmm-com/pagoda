{
  "name": "airone",
  "dockerComposeFile": [
    "../docker-compose.yml",
    "docker-compose.yml"
  ],
  "service": "airone",
  "workspaceFolder": "/workspace/airone",
  "postCreateCommand": "uv sync --extra dev && npm install",
  "customizations": {
    "vscode": {
      "extensions": [
        "ms-python.python",
        "ms-python.debugpy",
        "charliermarsh.ruff"
      ],
      "settings": {
        "python.defaultInterpreterPath": "/workspace/airone/.venv/bin/python"
      }
    }
  }
}