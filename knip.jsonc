{
  "entry": ["frontend/src/App.tsx"],
  "project": ["**/*.{js,ts,jsx,tsx}"],
  "ignore": ["docs/**/*.js", "static/**/*.js", "templates/**/*.js", "frontend/plugins/**/*"],
  "ignoreDependencies": [
    // CLI
    "webpack-bundle-analyzer",
    // used internally by eslint-plugin-import-x
    "eslint-import-resolver-typescript"
  ],
  "ignoreBinaries": [
    // to use `export` command
    "export"
  ]
}